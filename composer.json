{
  "name": "craftsman/cli",
  "description": "Craftsman CLI provides a number of helpful commands for your use while developing your Codeigniter application.",
  "keywords": [
    "craftsman",
    "codeigniter",
    "cli"
  ],
  "homepage": "https://github.com/davidsosavaldes/Craftsman",
  "license": "MIT",
  "authors": [
    {
      "name": "David Sosa Valdes",
      "email": "david.sosa.valdes@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "symfony/console": "^5.0",
    "symfony/filesystem": "^5.0",
    "symfony/process": "^5.0",
    "symfony/finder": "^5.4",
    "twig/twig" : "^3.0",
    "vlucas/phpdotenv": "^2.4",
    "psy/psysh": ">=0.8.11",
    "spatie/ray": "^1.34"
  },
  "autoload": {
    "psr-4": { "Craftsman\\": "src/" }
  },
  "support":{
    "issues": "https://github.com/davidsosavaldes/Craftsman/issues"
  },
  "bin": ["craftsman"],
  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  }
}
